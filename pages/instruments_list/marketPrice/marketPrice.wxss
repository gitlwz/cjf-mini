/* pages/instruments_list/productSale/productSale.wxss */
.content{
  width: 100%;
  height: 930rpx;
  background: #fff;
  position: relative;
}
ul{
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;

  /* background: #fff; */
}

.li{
  width:158rpx;
  height:60rpx;
  text-align: center;
  line-height: 60rpx;
  display: inline-block;
  background:rgba(245,245,245,1);
  border-radius:4px;
  margin-right: 15rpx;


}
.up-li{
  margin-top: 15rpx;
}
.active{
  background:rgba(208,161,108,0.2);
  color:#D0A16C;
}
.nav-head{
  height: 44rpx;
}
.nav-head label{
font-size:26rpx;
font-weight:400;
color:rgba(153,153,153,1);
}

.nav-img{
  position: absolute;
  right: 30rpx;
  top: 0;
}
.img-down-package{
  width: 120rpx;
  height: 100rpx;
  position: absolute;
  right: 0;
  top: 30rpx;
  background:linear-gradient(90deg,rgba(255,255,255,0) 0%,rgba(255,255,255,1) 39%,rgba(255,255,255,1) 100%);
  
}
.img-up-package{
  width: 120rpx;
  height: 60rpx;
  position: absolute;
  right: 0rpx;
  top: 30rpx;
z-index: 9999999;

}
.nav-up-img{
  position: absolute;
  right: 30rpx;
  top: 0;
z-index: 9999999;

}
.priceInfo{
font-size:26rpx;
font-weight:400;
color:rgba(153,153,153,1);
margin: 40rpx 0 10rpx;
}
.table-title{
  height: 93rpx;
  display: flex;
  align-items: center;
}
.title{
font-size:28rpx;
font-weight:500;
color:rgba(51,51,51,1);
}
.line{
  width:1px;
height:24rpx;
background: #DDDDDD;
margin: 0 15rpx;
display: inline-block;
}
.time{
  font-size:24rpx;
font-weight:400;
color:rgba(102,102,102,1);
}
.spans{
  display: flex;
  justify-content: space-around;
}
.span{
  display: inline-block;
  height: 80rpx;
  border-radius:4px;
  font-size:32rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(255,255,255,1);
text-align: center;
line-height: 80rpx;
}
.thead{
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #666666;

}
.tbody{
  height: 90rpx;
  display: flex;
  align-items: center;
}
/* .thead th{
    font-size:24rpx;
font-weight:400;
color:rgba(51,51,51,1);
} */
.price{
  display: inline-block;
  height: 90rpx;
  line-height: 90rpx;
}
.price0{
  width:130rpx;
  color: #EA5B28;
}
.price1{
  width:130rpx;
  color: #03C389;

}
.price3{
  width: 170rpx;
  text-align: center;
  color: #EA5B28;

}
.price4{
  text-align: right;
  color: #03C389;
}
.item0, .item2{
  background:linear-gradient(90deg,rgba(250,251,252,1) 0%,rgba(242,246,247,1) 100%);
}
.table-content{
  border-top:1px solid #F2F6F7;
}
.remark{
  text-align: center;
  position: relative;
  top: 6rpx;
  left: 0;
  right:0;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.exercisePrice{
  width: 160rpx;
  background:rgba(239,241,243,1);
  text-align:center;
  height: 100%;
}
