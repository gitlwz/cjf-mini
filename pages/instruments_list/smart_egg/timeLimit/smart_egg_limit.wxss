/* pages/products/productList/productList.wxss */
.jam-filter{
  display: flex;
  width: calc(100% - 60rpx);
  justify-content: space-between;
  margin: 0 30rpx 16rpx;
  flex-direction: row;
  align-items: center;
  padding-top: 30rpx;
}
.select-picker-view{
  flex: 1;
}
.area-selece-filter{
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
}

.area-selece-filter-left{
  margin-right: 20rpx;
}

.area-selece-filter-title{
  width:fit-content;
  margin-right: 15rpx;
  text-align: left;
  font-size:24rpx;
  color:#666666;
  line-height:40rpx;
}
.area-select-area {
  /*width: 190rpx;*/
  height: 56rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #f2f6f7;
  border-radius: 4px;
  justify-content: space-between;
  /*margin-right: 20rpx;*/
}
.area-select-area-name{
  font-size:24rpx;
  color:#333333;
  margin-left: 20rpx
}
.area-filter-icon{
  width: 28rpx;
  height: 28rpx;
  margin-right: 20rpx;
}

/*.group-date-filter{*/
