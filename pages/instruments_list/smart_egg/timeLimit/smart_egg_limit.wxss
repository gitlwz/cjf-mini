/*}*/

.group-date-filter{
  flex: 1;
}


.contract-line {
  width: 100%;
  height: 100%;
}

.contract-line .line {
  width: 100%;
  height: 450rpx;
}

.no-data-container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.no-content {
  width: 340rpx;
  height: 249rpx;
}
.no-data{
  font-size:24rpx;
  font-weight:400;
  color:#999999;
  line-height:40rpx;
}

.line-container {
  background-color: #ffffff;
  /* margin-top: 10rpx; */
  width: 100%;
  height: 100%;
}

.line-title {
  font-size: 28rpx;
  font-weight: 500;
  width: fit-content;
  color: #333;
  line-height: 44rpx;
}

.line-top-container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
  margin: 0 30rpx;
  border-bottom: 1rpx solid #F2F6F7;
}

.line-top-title-part {
  display: flex;
  align-items: 'center';
  width: 50%;
}

.land-scape-image{
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx
}
.line-legend-parent-container{
  display: flex;
  flex-direction: row;
  align-items: center;
  flex: 1;
  justify-content: space-between;
}
.line-legend-container {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.line-legend-line {
  width: 20rpx;
  height: 4rpx;
}

.line-legend-name {
  font-size: 24rpx;
  color: #333;
  line-height: 36rpx;
  margin-left: 8rpx;
}
.table-container{
  margin: 30rpx 30rpx;
  white-space: nowrap;
}

.table {
 width: 1405rpx;
  /* border: 1px solid gray; */
  /* border-right: none; */
  overflow:hidden;
}

.table-tr {
 /* border: 1px solid gray; */
 border-right: 1px solid #DDDDDD;
 border-top: 1px solid #DDDDDD;
 border-left: 1px solid #DDDDDD;
 display: flex;
 flex-direction: row;
 align-items: center;
 justify-content: center;
 width: 1274rpx;
 height: 46rpx;
}
.table-header {
  background:#DDDDDD;
  font-size:20rpx;
  font-weight:400;
  color:#999999;
}

.table-th {
  border-right: 1px solid #DDDDDD;
  /* border-top: 1px solid gray;
  border-left: 1px solid gray; */
  font-size: 20rpx;
  text-align: center;
  vertical-align: middle;
  width: 98rpx;
}

.table-td {
  border-right: 1px solid #DDDDDD;
  /* border-top: 1px solid gray;
  border-left: 1px solid gray; */
    font-size: 20rpx;
  text-align: center;
  vertical-align: middle;
  width: 98rpx;
}

.th-end{
  background-color: #f00;
  border-right: none;
}

.table-tr-end {
  border-bottom:  1px solid #DDDDDD;
}

.table-tcolumn-first {
  width: 98rpx;
  justify-content: center;
  border-right: 1px solid #DDDDDD;
}
