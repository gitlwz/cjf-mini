/* components/flow3/flow3.wxss */
.content-container{
  background:#fff;
  position:relative;
  margin-top: 12rpx;
  padding: 60rpx 40rpx 0;
}
.item{
width: fit-content;
}
.listItem{
  margin-bottom: 40rpx;
}
.itemContent{
font-size:26rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(51,51,51,1);
line-height:42rpx;
width: 610rpx;
margin-left: 18rpx;
display: inline-block;
}
.back{
  position: absolute;
  bottom:40rpx;
  width:670rpx;
height:90rpx;
background:#D0A16C;border-radius:16rpx;
line-height:90rpx;
text-align: center;
font-size:32rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(255,255,255,1);
}
.index{
  width:42rpx;
height:42rpx;
background:rgba(208,161,108,0.2);
font-size:28rpx;
font-weight:bold;
color:rgba(162,101,66,1);
line-height:42rpx;
text-align: center;
display: inline-block;
border-radius:50%;
vertical-align: top;
margin-top: 4rpx;
}
